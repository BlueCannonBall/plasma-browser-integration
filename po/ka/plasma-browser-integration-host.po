#: downloadjob.cpp:138
#, kde-format
msgid ""
"This type of file can harm your computer. If you want to keep it, accept "
"this download from the browser window."
msgstr ""
"ამ ტიპის ფაილს თქვენი კომპიუტერის ვნება შეუძლია. თუ გნებავთ, შეინახოთ ის, "
"დაეთანხმეთ გადმოწერას ბრაუზერის ფანჯრიდან."

#: downloadjob.cpp:235
#, kde-format
msgid "Access denied."
msgstr "წვდომა აკრძალულია."

#: downloadjob.cpp:236
#, kde-format
msgid "Insufficient free space."
msgstr "დისკზე არასაკმარისი ადგილია."

#: downloadjob.cpp:237
#, kde-format
msgid "The file name you have chosen is too long."
msgstr "ფაილის არჩეული სახელი ძალიან გრძელია."

#: downloadjob.cpp:238
#, kde-format
msgid "The file is too large to be downloaded."
msgstr "ფაილი გადმოსაწერად ძალიან დიდია."

#: downloadjob.cpp:240
#, kde-format
msgid "The file possibly contains malicious contents."
msgstr "ფაილის შემცველობა დიდი ალბათობით საეჭვოა."

#: downloadjob.cpp:241
#, kde-format
msgid "A temporary error has occurred. Please try again later."
msgstr "დროებითი შეცდომა. მოგვიანებით სცადეთ."

#: downloadjob.cpp:243
#, kde-format
msgid "A network error has occurred."
msgstr "ქსელური შეცდომა."

#: downloadjob.cpp:244
#, kde-format
msgid "The network operation timed out."
msgstr "ქსელური ოპერაციის ვადა გავიდა."

#: downloadjob.cpp:245
#, kde-format
msgid "The network connection has been lost."
msgstr "ქსელური შეერთების შეცდომა."

#: downloadjob.cpp:246
#, kde-format
msgid "The server is no longer reachable."
msgstr "სერვერი მიუწვდომელია."

#: downloadjob.cpp:248
#, kde-format
msgid "A server error has occurred."
msgstr "სერვერის შეცდომა."

#: downloadjob.cpp:252
#, kde-format
msgid "The server does not have the requested data."
msgstr "სერვერს მოთხოვნილი მონაცემები არ გააჩნია."

#: downloadjob.cpp:254
#, kde-format
msgid "The browser application closed unexpectedly."
msgstr "ბრაუზერის აპლიკაცია მოულოდნელად დაიხურა."

#: downloadjob.cpp:260
#, kde-format
msgid "An unknown error occurred while downloading."
msgstr "გადმოწერის უცნობი შეცდომა."

#: downloadjob.cpp:298
#, kde-format
msgctxt "Job heading, like 'Copying'"
msgid "Downloading"
msgstr "გადმოწერა"

#: downloadjob.cpp:299
#, kde-format
msgctxt "The URL being downloaded"
msgid "Source"
msgstr "წყარო"

#: downloadjob.cpp:300
#, kde-format
msgctxt "The location being downloaded to"
msgid "Destination"
msgstr "დანიშნულება"

#: historyrunnerplugin.cpp:108
#, kde-format
msgctxt "Dummy search result"
msgid "Additional permissions are required"
msgstr "საჭიროა დამატებითი წვდომები"

#: tabsrunnerplugin.cpp:38
#, kde-format
msgid "Mute Tab"
msgstr "ჩანართის დადუმება"

#: tabsrunnerplugin.cpp:43
#, kde-format
msgid "Unmute Tab"
msgstr "ჩანართის დადუმების გამორთვა"
