printf ", '%s'" "$@"
printf '\n')

gdbus call \
    --session \
    --dest org.kde.plasma.browser.integration \
    --object-path /org/kde/plasma/browser/integration \
    --method org.kde.plasma.browser.integration.FlatpakIntegrator.Link \
    ["$args"] 0 1 2 > /dev/stderr

